package server;

import java.io.Serializable;

public class ReflectionParent implements Serializable {

	/**
	 * Every Logic class that the user can call needs to extend this class
	 */
	public ReflectionParent() {
		// TODO Auto-generated constructor stub
	}
	
	public Class[] getClasses(){
	return this.getClass().getClasses();
	}

}
